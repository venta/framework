{
    "name": "abava/cache",
    "description": "The Abava Caching package",
    "authors": [
        {
            "name": "Konstantins Zukovs",
            "email": "kz@scandiweb.com"
        },
        {
            "name": "Alexey Rehov",
            "email": "aleksejs@scandiweb.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Abava\\Cache\\": ""
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "mockery/mockery": "^0.9.5"
    },
    "require": {
        "psr/cache": "^1.0",
        "cache/adapter-common": "^0.3",
        "cache/hierarchical-cache": "^0.2",
        "cache/taggable-cache": "^0.4",
        "cache/namespaced-cache": "^0.1",
        "cache/array-adapter": "^0.4"
    },
    "suggest": {
        "cache/filesystem-adapter": "Filesystem cache adapter",
        "cache/redis-adapter": "Redis cache adapter"
    }
}
