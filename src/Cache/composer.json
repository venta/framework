{
    "name": "venta/cache",
    "description": "The Venta Cache package.",
    "keywords": [
        "venta",
        "cache"
    ],
    "authors": [
        {
            "name": "Venta Core Team",
            "email": "team@venta.io"
        },
        {
            "name": "Venta Community",
            "homepage": "https://github.com/venta/cache/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/venta/framework/issues"
    },
    "require": {
        "php": "^7.0",
        "venta/contracts": "dev-master",
        "cache/adapter-common": "^0.3",
        "cache/hierarchical-cache": "^0.2",
        "cache/taggable-cache": "^0.4",
        "cache/namespaced-cache": "^0.1",
        "cache/array-adapter": "^0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "mockery/mockery": "^0.9.5"
    },
    "suggest": {
        "cache/filesystem-adapter": "Filesystem cache adapter",
        "cache/redis-adapter": "Redis cache adapter"
    },
    "autoload": {
        "psr-4": {
            "Venta\\Cache\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
