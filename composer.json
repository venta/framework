{
    "name": "venta/framework",
    "description": "Venta Framework package",
    "authors": [
        {
            "name": "Konstantins Zukovs",
            "email": "kz@scandiweb.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/abava/container.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/abava/http.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/abava/routing.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/abava/console.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Venta\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "mockery/mockery": "^0.9.5"
    },
    "require": {
        "abava/container": "dev-master",
        "abava/http": "dev-master",
        "abava/routing": "dev-master",
        "abava/console": "dev-master",
        "symfony/console": "^3.1",
        "filp/whoops": "^2.1",
        "vlucas/phpdotenv": "^2.3",
        "monolog/monolog": "^1.19"
    }
}
