{
    "name": "venta/console",
    "description": "The Venta Console package.",
    "keywords": [
        "venta",
        "console"
    ],
    "authors": [
        {
            "name": "Venta Core Team",
            "email": "team@venta.io"
        },
        {
            "name": "Venta Community",
            "homepage": "https://github.com/venta/console/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/venta/framework/issues"
    },
    "require": {
        "php": "^7.0",
        "venta/contracts": "dev-master",
        "symfony/console": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "Venta\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "stub\\Venta\\Console\\": "stub"
        }
    },
    "minimum-stability": "dev"
}
